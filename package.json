{
  "name": "ice-spear-editor",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "main": "main.js",
  "bin": {
    "ice-spear": "./main.js"
  },
  "scripts": {
    "start": "electron . --js-flags=\"--expose-gc\"",
    "update-license-csv": "license-checker --csv > MODULES_LICENSE.csv",
    "test": "electron-mocha './{,!(node_modules)/**/}*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "gitlab:ice-spear-tools/ice-spear"
  },
  "ice-spear": {
    "debug-mode": true,
    "default-app": "launcher",
    "app-icon": "assets/icons/icon_256.png",
    "default-size": [
      1024,
      768
    ]
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "electron-mocha": "^5.0.0",
    "mocha": "^5.0.4"
  },
  "dependencies": {
    "ace-builds": "^1.3.3",
    "binary-file": "gitlab:ice-spear-tools/binary-file",
    "byaml-lib": "gitlab:ice-spear-tools/byaml-lib",
    "crc32": "^0.2.2",
    "dateformat": "^3.0.3",
    "delete": "^1.1.0",
    "electron": "^2.0.0-beta.3",
    "electron-remote": "^1.2.0",
    "fs-extra": "^5.0.0",
    "int24": "0.0.1",
    "izitoast": "^1.3.0",
    "pako": "^1.0.6",
    "png-lib": "gitlab:ice-spear-tools/png-lib",
    "sanitize-filename": "^1.6.1",
    "sarc-lib": "gitlab:ice-spear-tools/sarc-lib",
    "split.js": "^1.3.5",
    "sweetalert2": "^7.19.2",
    "three-orbit-controls": "^82.1.0",
    "uuid": "^3.2.1",
    "yaz0-lib": "gitlab:ice-spear-tools/yaz0-lib"
  }
}
